{
    "name": "laminas-api-tools/api-tools-hal",
    "description": "Laminas Module providing Hypermedia Application Language assets and rendering",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "api-tools",
        "module",
        "rest",
        "hal"
    ],
    "homepage": "https://api-tools.getlaminas.org",
    "support": {
        "docs": "https://api-tools.getlaminas.org/documentation",
        "issues": "https://github.com/laminas-api-tools/api-tools-hal/issues",
        "source": "https://github.com/laminas-api-tools/api-tools-hal",
        "rss": "https://github.com/laminas-api-tools/api-tools-hal/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=5.3.3",
        "laminas-api-tools/api-tools-api-problem": "dev-master",
        "laminas/laminas-eventmanager": "dev-develop",
        "laminas/laminas-json": "dev-develop",
        "laminas/laminas-mvc": "dev-develop",
        "laminas/laminas-paginator": "dev-develop",
        "laminas/laminas-stdlib": "dev-develop",
        "laminas/laminas-uri": "dev-develop",
        "laminas/laminas-view": "dev-develop",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "laminas/laminas-console": "dev-develop",
        "laminas/laminas-http": "dev-develop",
        "laminas/laminas-loader": "dev-develop",
        "laminas/laminas-servicemanager": "dev-develop",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "Laminas\\ApiTools\\Hal\\": "src/",
            "LaminasTest\\ApiTools\\": "test/"
        }
    },
    "replace": {
        "zfcampus/zf-hal": "self.version"
    }
}
