{
    "name": "laminas-api-tools/api-tools-hal",
    "description": "Laminas Module providing Hypermedia Application Language assets and rendering",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "api-tools",
        "module",
        "rest",
        "hal"
    ],
    "homepage": "https://api-tools.getlaminas.org",
    "support": {
        "docs": "https://api-tools.getlaminas.org/documentation",
        "issues": "https://github.com/laminas-api-tools/api-tools-hal/issues",
        "source": "https://github.com/laminas-api-tools/api-tools-hal",
        "rss": "https://github.com/laminas-api-tools/api-tools-hal/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev",
            "dev-develop": "1.2-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "laminas-api-tools/api-tools-api-problem": "~1.0",
        "laminas/laminas-escaper": ">=2.3,<2.5",
        "laminas/laminas-eventmanager": ">=2.3,<2.5",
        "laminas/laminas-json": ">=2.3,<2.5",
        "laminas/laminas-mvc": ">=2.3,<2.5",
        "laminas/laminas-paginator": ">=2.3,<2.5",
        "laminas/laminas-stdlib": ">=2.3,<2.5",
        "laminas/laminas-uri": ">=2.3,<2.5",
        "laminas/laminas-view": ">=2.3,<2.5",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "laminas/laminas-console": ">=2.3,<2.5",
        "laminas/laminas-filter": ">=2.3,<2.5",
        "laminas/laminas-http": ">=2.3,<2.5",
        "laminas/laminas-loader": ">=2.3,<2.5",
        "laminas/laminas-servicemanager": ">=2.3,<2.5",
        "laminas/laminas-validator": ">=2.3,<2.5",
        "phpunit/phpunit": "4.7.*",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\ApiTools\\Hal\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\ApiTools\\Hal\\": "test/"
        }
    },
    "replace": {
        "zfcampus/zf-hal": "self.version"
    }
}
