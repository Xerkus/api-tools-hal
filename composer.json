{
    "name": "laminas-api-tools/api-tools-hal",
    "description": "Laminas Module providing Hypermedia Application Language assets and rendering",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "api-tools",
        "module",
        "rest",
        "hal"
    ],
    "homepage": "https://api-tools.getlaminas.org",
    "support": {
        "docs": "https://api-tools.getlaminas.org/documentation",
        "issues": "https://github.com/laminas-api-tools/api-tools-hal/issues",
        "source": "https://github.com/laminas-api-tools/api-tools-hal",
        "rss": "https://github.com/laminas-api-tools/api-tools-hal/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require": {
        "php": ">=5.4.8",
        "laminas-api-tools/api-tools-api-problem": "~1.0-dev",
        "laminas/laminas-escaper": "~2.3-dev",
        "laminas/laminas-eventmanager": "~2.3-dev",
        "laminas/laminas-json": "~2.3-dev",
        "laminas/laminas-mvc": "~2.3-dev",
        "laminas/laminas-paginator": "~2.3-dev",
        "laminas/laminas-stdlib": "~2.3-dev",
        "laminas/laminas-uri": "~2.3-dev",
        "laminas/laminas-view": "~2.3-dev",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "laminas/laminas-console": "~2.3-dev",
        "laminas/laminas-http": "~2.3-dev",
        "laminas/laminas-loader": "~2.3-dev",
        "laminas/laminas-servicemanager": "~2.3-dev",
        "laminas/laminas-validator": "~2.3-dev",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "Laminas\\ApiTools\\Hal\\": "src/",
            "LaminasTest\\ApiTools\\Hal\\": "test/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "replace": {
        "zfcampus/zf-hal": "self.version"
    }
}
